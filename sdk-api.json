  "components": {
    "schemas": {
      "EmptyRequest": {
        "type": "object",
        "description": "Empty request (for endpoints that don't need parameters)"
      },
      "PerpCandleRequest": {
        "type": "object",
        "description": "Request for perp candle snapshot",
        "required": [
          "dex",
          "candleInterval"
        ],
        "properties": {
          "candleInterval": {
            "type": "integer",
            "format": "int32",
            "description": "Candle interval: 0:hour, 1:day, 2:month, 3:minute, 4:15minutes",
            "example": 0,
            "maximum": 4,
            "minimum": 0
          },
          "dex": {
            "type": "string",
            "description": "Market name (e.g., \"BTC-USDT\")",
            "example": "BTC-USDT"
          },
          "end": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "description": "End time for the query (milliseconds timestamp)"
          },
          "limit": {
            "type": "integer",
            "format": "int32",
            "description": "Maximum number of records to return (1-5000)",
            "example": 500,
            "maximum": 5000,
            "minimum": 1
          },
          "start": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "description": "Start time for the query (milliseconds timestamp)"
          }
        }
      },
      "PerpMarketByIdRequest": {
        "type": "object",
        "description": "Request for getting a specific perp market by ID",
        "required": [
          "marketId"
        ],
        "properties": {
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          }
        }
      },
      "PerpOrderByTxRequest": {
        "type": "object",
        "description": "Request for perp order by transaction hash",
        "required": [
          "txHash"
        ],
        "properties": {
          "txHash": {
            "type": "string",
            "description": "Transaction hash",
            "example": "0xabcdef1234567890abcdef1234567890abcdef1234567890abcdef1234567890"
          }
        }
      },
      "PerpOrderRequest": {
        "type": "object",
        "description": "Request for perp order by ID",
        "required": [
          "user",
          "marketId",
          "oid"
        ],
        "properties": {
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          },
          "oid": {
            "type": "integer",
            "format": "int32",
            "description": "Order ID",
            "example": 12345,
            "minimum": 0
          },
          "user": {
            "type": "string",
            "description": "User wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb"
          }
        }
      },
      "PerpUserFillsByTimeRequest": {
        "type": "object",
        "description": "Request for perp user fills by time",
        "required": [
          "user",
          "marketId"
        ],
        "properties": {
          "end": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "description": "End time for the query (milliseconds timestamp)"
          },
          "limit": {
            "type": "integer",
            "format": "int32",
            "description": "Maximum number of records to return (1-1000)",
            "example": 100,
            "maximum": 1000,
            "minimum": 1
          },
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          },
          "start": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "description": "Start time for the query (milliseconds timestamp)"
          },
          "user": {
            "type": "string",
            "description": "User wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb"
          }
        }
      },
      "PerpUserFillsRequest": {
        "type": "object",
        "description": "Request for perp user fills",
        "required": [
          "user",
          "marketId"
        ],
        "properties": {
          "limit": {
            "type": "integer",
            "format": "int32",
            "description": "Maximum number of records to return (1-1000)",
            "example": 100,
            "maximum": 1000,
            "minimum": 1
          },
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          },
          "user": {
            "type": "string",
            "description": "User wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb"
          }
        }
      },
      "PerpUserOrdersRequest": {
        "type": "object",
        "description": "Request for perp user orders (open or historical)",
        "required": [
          "user",
          "marketId"
        ],
        "properties": {
          "limit": {
            "type": "integer",
            "format": "int32",
            "description": "Maximum number of records to return (1-1000)",
            "example": 100,
            "maximum": 1000,
            "minimum": 1
          },
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          },
          "user": {
            "type": "string",
            "description": "User wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb"
          }
        }
      },
      "PerpVolumeRequest": {
        "type": "object",
        "description": "Request for perp volume statistics",
        "required": [
          "marketId"
        ],
        "properties": {
          "marketId": {
            "type": "integer",
            "format": "int32",
            "description": "Market ID",
            "example": 1,
            "minimum": 0
          },
          "period": {
            "type": "string",
            "description": "Time period: 1h, 24h, 7d, or 30d",
            "example": "24h"
          }
        }
      },
      "SpotCandleRequest": {
        "type": "object",
        "description": "Request for spot candle snapshot",
        "required": [
          "dex",
          "candleInterval"
        ],
        "properties": {
          "candleInterval": {
